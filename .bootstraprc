{
  "bootstrapVersion": 4,
  // Bootstrap variables that get imported before original Bootstrap variables
  "preBootstrapCustomizations": "./client/assets/scss/bootstrap/customizations/preBootstrapCustomizations.scss",
  // customize Bootstrap variables based on the values established in the Bootstrap _variables.scss file
  "bootstrapCustomizations": "./client/assets/scss/bootstrap/customizations/bootstrapCustomizations.scss",
  // custom styles. @imports of application styles
  "appStyles": "./client/assets/scss/app/app3.scss",
  // Outputs debugging info
  "loglevel": "disabled",
  // NODE_ENV environment variable
  // Array of webpack loaders
  // Extract styles to css file (extract plugin)
  "env": {
    "development": {
      "styleLoaders": [
        "style-loader?sourceMap",
        "css-loader?sourceMap&importLoaders=2",
        "resolve-url-loader",
        "sass-loader?sourceMap",
      ],
      "extractStyles": false
    },
    "production": {
      "styleLoaders": [
        "style-loader",
        "css-loader?importLoaders=1",
        "sass-loader?sourceMap",
      ],
      "extractStyles": true,
    }
  },
  // Bootstrap scripts (Default: all)
  // "scripts": true / false // enable/disable all scripts
  "scripts": {
    "alert": true,
    "button": true,
    "carousel": true,
    "collapse": true,
    "dropdown": true,
    "modal": true,
    "popover": true,
    "scrollspy": true,
    "tab": true,
    "tooltip": true,
    "util": true
  },
  // Bootstrap styles (Default: all)
  // "styles": true / false // enable/disable all scripts
  "styles": {
    "alert": true,
    "badge": true,
    "breadcrumb": true,
    "button-group": true,
    "buttons": true,
    "card": true,
    "carousel": true,
    "close": true,
    "code": true,
    "custom-forms": true,
    "dropdown": true,
    "forms": true,
    "functions": true,
    "grid": true,
    "images": true,
    "input-group": true,
    "jumbotron": true,
    "list-group": true,
    "media": true,
    "mixins": true,
    "modal": true,
    "nav": true,
    "navbar": true,
    "pagination": true,
    "popover": true,
    "print": true,
    "progress": true,
    "reboot": true,
    "root": true,
    "tables": true,
    "tooltip": true,
    "transitions": true,
    "type": true,
    "utilities": true
  }
}
